{
  "expo": {
    "name": "Edaiva",
    "slug": "edaivajobsApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },

      "package": "com.prateeksri.edaivajobsapp",
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./src/assets/icon.png"
    },

    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "edaiva-test",
            "project": "c141c87ead5b",
            "authToken": "0ac9467abadd4478915f3be1b980fe454004c168520b40748edb2610438fbcd5"
          }
        }
      ]
    },
    "plugins": [
      "sentry-expo",
      [
        "expo-notifications",
        {
          "icon": "./src/assets/notification-icon.png",
          "color": "#28B1E6"
        }
      ]
    ],
    "scheme": "com.prateeksri.edaivajobsapp"
  }
}
